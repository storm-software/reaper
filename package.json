{
  "name": "@storm-trading/monorepo",
  "version": "0.0.0",
  "description": "ðŸ’µ A monorepo containing various utilities, libraries, and applications used by Storm Software in trading processes",
  "repository": {
    "type": "github",
    "url": "https://github.com/storm-software/storm-trading.git"
  },
  "homepage": "https://stormsoftware.com",
  "bugs": {
    "url": "https://github.com/storm-software/storm-trading/issues",
    "email": "support@stormsoftware.com"
  },
  "author": {
    "name": "Storm Software",
    "email": "contact@stormsoftware.com",
    "url": "https://stormsoftware.com/team"
  },
  "maintainers": [
    {
      "name": "Storm Software",
      "email": "contact@stormsoftware.com",
      "url": "https://stormsoftware.com/team"
    },
    {
      "name": "Pat Sullivan",
      "email": "admin@stormsoftware.com",
      "url": "https://patsullivan.org"
    }
  ],
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/storm-software"
  },
  "license": "Not Applicable",
  "private": true,
  "packageManager": "pnpm@9.6.0",
  "engines": {
    "node": ">=20.11.0",
    "pnpm": ">=9.6.0"
  },
  "keywords": [
    "storm",
    "storm-ops",
    "storm-stack",
    "acidic",
    "cyclone-ui",
    "rust",
    "foundry",
    "sullivanpj",
    "monorepo"
  ],
  "scripts": {
    "adr": "pnpm log4brains adr new",
    "adr-preview": "pnpm log4brains preview",
    "build": "nx affected -t build --parallel=3",
    "build-all": "nx run-many -t build --all --parallel=3",
    "clean": "rimraf dist",
    "clean-packages": "rimraf dist/packages",
    "commit": "pnpm exec storm-git commit",
    "dep-graph": "nx dep-graph",
    "e2e": "nx e2e",
    "format": "nx format:write",
    "format-prettier": "pnpm exec prettier --write --ignore-unknown --no-error-on-unmatched-pattern --ignore-path=\"@storm-software/prettier/.prettierignore\" --cache --cache-location=\"./node_modules/.cache/prettier\" ",
    "format-readme": "pnpm storm-git readme-gen --templates=\"./tools/readme-templates\"",
    "format-toml": "pnpm exec taplo format --config=\"@storm-software/linting-tools/taplo/.taplo.toml\" --cache-path=\"./tmp/taplo\"",
    "help": "nx help",
    "lint": "pnpm exec storm-lint all --skip-cspell --skip-alex --skip-circular-deps",
    "lint-ls": "pnpm exec ls-lint --config=\"@storm-software/linting-tools/ls-lint/ls-lint.yml\"",
    "lint-prettier": "pnpm exec prettier --check --ignore-path=\"@storm-software/prettier/.prettierignore\" --cache ",
    "lint-toml": "pnpm exec taplo format --check --config=\"@storm-software/linting-tools/taplo/.taplo.toml\"",
    "local-registry": "nx local-registry @storm-trading/monorepo",
    "nuke": "nx clear-cache && rimraf tmp && rimraf node_modules/.cache/nx && rimraf **/node_modules && pnpm clear",
    "nx": "nx",
    "prebuild": "echo Skipping prebuild...",
    "prebuild-all": "echo Skipping prebuild-all...",
    "rebuild": "pnpm clean && pnpm build",
    "rebuild-all": "pnpm clean-all && pnpm build-all",
    "release": "pnpm exec storm-git release",
    "start": "nx serve",
    "test": "echo Skipping all workspace testing..."
  },
  "dependencies": {
    "@ls-lint/ls-lint": "2.2.3",
    "@ltd/j-toml": "1.38.0",
    "@monodon/rust": "1.4.0",
    "@nx/devkit": "19.5.6",
    "@nx/esbuild": "19.5.6",
    "@nx/eslint": "19.5.6",
    "@nx/eslint-plugin": "19.5.6",
    "@nx/jest": "19.5.6",
    "@nx/js": "19.5.6",
    "@nx/plugin": "19.5.6",
    "@nx/workspace": "19.5.6",
    "@storm-software/config-tools": "latest",
    "@storm-software/eslint": "latest",
    "@storm-software/git-tools": "latest",
    "@storm-software/linting-tools": "latest",
    "@storm-software/prettier": "latest",
    "@storm-software/terraform-tools": "latest",
    "@storm-software/testing-tools": "latest",
    "@storm-software/workspace-tools": "latest",
    "@taplo/cli": "0.7.0",
    "@types/jest": "29.5.12",
    "@types/node": "^20.14.10",
    "conventional-changelog-conventionalcommits": "8.0.0",
    "copyfiles": "2.4.1",
    "esbuild": "0.21.5",
    "esbuild-register": "3.5.0",
    "eslint": "9.5.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-environment-node": "29.7.0",
    "knip": "5.25.2",
    "lefthook": "1.6.18",
    "nx": "19.5.6",
    "nx-cloud": "19.0.0",
    "prettier": "3.3.2",
    "rimraf": "5.0.7",
    "sherif": "0.10.0",
    "ts-jest": "29.1.5",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "tslib": "^2.6.3",
    "typescript": "5.5.3",
    "verdaccio": "5.31.1"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.6.4",
    "@nx/node": "19.5.6"
  },
  "devEngines": {
    "node": "20.x || 21.x"
  },
  "namespace": "@storm-trading",
  "nx": {
    "includedScripts": [
      "lint-ls",
      "lint",
      "format",
      "format-readme",
      "format-prettier",
      "format-toml",
      "commit",
      "release"
    ]
  },
  "prettier": "@storm-software/prettier/config.json",
  "triggerEmptyDevReleaseByIncrementingThisNumber": 0
}
