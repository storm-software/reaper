{
  "name": "reaper-eth-order-telegram-bot",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/eth-order-telegram-bot/src",
  "targets": {
    "build-local": {
      "executor": "@storm-software/workspace-tools:cargo-build",
      "outputs": ["{workspaceRoot}/dist/target/apps/eth-order-telegram-bot"],
      "dependsOn": ["^build"],
      "options": {
        "outputPath": "dist/target/apps/eth-order-telegram-bot"
      },
      "defaultConfiguration": "development",
      "configurations": {
        "development": {
          "profile": "dev"
        },
        "production": {
          "profile": "prod"
        }
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "inputs": ["rust", "^production"],
      "outputs": ["{workspaceRoot}/dist/target/apps/eth-order-telegram-bot"],
      "dependsOn": ["^build"],
      "options": {
        "commands": [
          "cargo lambda build --profile prod --output-format zip --target-dir ../../dist/target/apps/eth-order-telegram-bot"
        ],
        "color": true,
        "cwd": "apps/eth-order-telegram-bot"
      }
    }
  },
  "tags": []
}
